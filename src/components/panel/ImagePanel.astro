---
import type {Polymorphic} from "astro/types";

import type {AttributeProps as PanelAttributeProps} from "./Panel.astro";
import ContentPanel from "./ContentPanel.astro";

type _Props = Omit<Polymorphic<{
  "as": "img";
} & PanelAttributeProps>, "as">;

export type Props = _Props;

const {...props} = Astro.props;
---

<ContentPanel as="img" {...props} />