---
import type {Polymorphic} from "astro/types";

import Panel, {type AttributeProps as PanelAttributeProps} from "./Panel.astro";

// We can't make this exported directly. Thanks ESBuild :)
type _Props = Omit<Polymorphic<{
  "as": "img";
} & PanelAttributeProps>, "as">;

export type Props = _Props;

const {...props} = Astro.props;
---

<Panel as="img" {...props} />